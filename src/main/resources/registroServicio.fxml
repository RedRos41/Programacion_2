<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="co.edu.uniquindio.clinica.controladores.RegistroServicioControlador">

    <TextField fx:id="nombreServicio" promptText="Nombre del Servicio"/>
    <TextField fx:id="precioServicio" promptText="Precio del Servicio"/>
    <Button text="Registrar Servicio" onAction="#registrarServicio"/>
</VBox>
